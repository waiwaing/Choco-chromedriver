    <projectUrl>https://chromedriver.chromium.org/home</projectUrl>
    <packageSourceUrl>https://github.com/yetanotherchris/Choco-chromedriver-latest</packageSourceUrl>
    <tags>chromedriver webdriver chrome</tags>
    <copyright>Chromedriver authors</copyright>
    <licenseUrl>https://chromium.googlesource.com/chromium/src/+/refs/heads/main/chrome/test/chromedriver/BUILD.gn</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://cdn.rawgit.com/yetanotherchris/Choco-chromedriver-latest/master/icon.png</iconUrl>
    <releaseNotes></releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
